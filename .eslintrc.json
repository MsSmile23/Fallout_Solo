{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "import/no-extraneous-dependencies": 0,
        "react/prop-types": 0,
        "no-alert": 0,
        "no-console": 0
    }
}
